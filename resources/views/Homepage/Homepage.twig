    <script2>
        var $carousel_1 = $('#wrap .carousel').flickity({
        lazy: true,
        imagesLoaded: true,
        wrapAround: true,
        prevNextButtons: true,
        fade: true,
        autoPlay: 3010,
        pauseAutoPlayOnHover: false
        });
        $(document).ready(function () {
        var $carouselStatus = $('.carousel-status');
        var flkty = $carousel_1.data('flickity');
        var carouselStatus = document.querySelector('.carousel-status');

        function updateStatus() {
        $(".flickity-slider .content ~ a").removeClass('animateNow');
        var cellNumber = flkty.selectedIndex + 1;
        if (cellNumber == 1) {

        $(".carousel button[aria-label^='Next']").css("display", "block");

        $(".carousel button[aria-label^='Previous']").css("display", "block");
        if ('{{ slide1pos }}' == '1') {
        document.getElementById("wrap").style.background = '{{ slide1bgcolor }}';
        } else if ('{{ slide1pos }}' == '2') {
        document.getElementById("wrap").style.background = '{{ slide2bgcolor }}';
        } else if ('{{ slide1pos }}' == '3') {
        document.getElementById("wrap").style.background = '{{ slide3bgcolor }}';
        } else if ('{{ slide1pos }}' == '4') {
        document.getElementById("wrap").style.background = '{{ slide4bgcolor }}';
        }
        } else if (cellNumber == '2') {
        if ('{{ slide2pos }}' == '1') {
        document.getElementById("wrap").style.background = '{{ slide1bgcolor }}';
        } else if ('{{ slide2pos }}' == '2') {
        document.getElementById("wrap").style.background = '{{ slide2bgcolor }}';
        } else if ('{{ slide2pos }}' == '3') {
        document.getElementById("wrap").style.background = '{{ slide3bgcolor }}';
        } else if ('{{ slide2pos }}' == '4') {
        document.getElementById("wrap").style.background = '{{ slide4bgcolor }}';
        }
        } else if (cellNumber == 3) {
        if ('{{ slide3pos }}' == '1') {
        document.getElementById("wrap").style.background = '{{ slide1bgcolor }}';
        } else if ('{{ slide3pos }}' == '2') {
        document.getElementById("wrap").style.background = '{{ slide2bgcolor }}';
        } else if ('{{ slide3pos }}' == '3') {
        document.getElementById("wrap").style.background = '{{ slide3bgcolor }}';
        } else if ('{{ slide3pos }}' == '4') {
        document.getElementById("wrap").style.background = '{{ slide4bgcolor }}';
        }
        } else if (cellNumber == 4) {
        if ('{{ slide4pos }}' == '1') {
        document.getElementById("wrap").style.background = '{{ slide1bgcolor }}';
        } else if ('{{ slide4pos }}' == '2') {
        document.getElementById("wrap").style.background = '{{ slide2bgcolor }}';
        } else if ('{{ slide4pos }}' == '3') {
        document.getElementById("wrap").style.background = '{{ slide3bgcolor }}';
        } else if ('{{ slide4pos }}' == '4') {
        document.getElementById("wrap").style.background = '{{ slide4bgcolor }}';
        }
        }
        }
        function addAnim(){
        $(".flickity-slider .content ~ a").addClass('animateNow');
        }
        function removeAnim() {
        $(".flickity-slider .content ~ a").removeClass('animateNow');
        $carousel_1.flickity('playPlayer')
        }

        $carousel_1.on('scroll.flickity', removeAnim);
        updateStatus();
        $carousel_1.on('settle.flickity', addAnim);
        $carousel_1.on('select.flickity', removeAnim);
        $carousel_1.on('change.flickity', updateStatus);
        $("#wrap").addClass("pageloaded");

        });
    </script2>



