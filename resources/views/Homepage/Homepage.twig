                                    <div class="text-white mr-1">
                                        {{ trans("devMlt::Template.FlashSaleEnds") }}
                                    </div>
                                    <div id="tb1-days">00</div>
                                    <span>:</span>
                                    <div id="tb1-hours">00</div>
                                    <span>:</span>
                                    <div id="tb1-minutes">00</div>
                                    <span>:</span>
                                    <div id="tb1-seconds">00</div>
                                </div>
                            </div>
                            <script2>
                                var start_date1 = new Date("{{ flash1itemtime|date("Y-m-d") }}T00:00:00+02:00"),
                                countDown2;
                                var today = new Date();
                                var Difference_In_Time = start_date1.getTime() - today.getTime();
                                var Difference_In_Days = Difference_In_Time / (1000 * 3600 * 24);
                                if (Difference_In_Days > 0) {
                                $('#flash_sale').show();
                                $('#beliebte_teppich').show();
                                }
                                (function ($) {
                                $(document).ready(function () {
                                waitForElementToDisplay1_2('#tb1-days', 40, 200, startCountDown_1);
                                });
                                })(jQuery);

                                function waitForElementToDisplay1_2(selector, time, tries, callFunc) {
                                if (document.querySelector(selector) !== null) {
                                callFunc();
                                } else {
                                if (tries >= 1) {
                                setTimeout(function () {
                                waitForElementToDisplay1_2(selector, time, tries - 1, callFunc);
                                }, time);
                                } else {
                                return;
                                }
                                }
                                }

                                function startCountDown_1() {
                                countDown_1 = setInterval(function () {
                                countDownTimer_1((start_date1 - new Date()) / 1000);
                                }, 500);
                                }

                                var last_day,
                                last_hour,
                                last_minute,
                                last_second;

                                function countDownTimer_1(seconds) {
                                var rem_days = Math.floor((seconds / 86400) % 60),
                                days = (rem_days * 86400),
                                rem_hours = Math.floor(((seconds - days) / 3600) % 60),
                                hours = (rem_days * 86400) + (rem_hours * 3600),
                                rem_minutes = Math.floor((seconds - hours) / 60),
                                rem_seconds = Math.floor(seconds % 60);
                                $('#tb1-days').text(pad(rem_days));
                                $('#tb1-hours').text(pad(rem_hours));
                                $('#tb1-minutes').text(pad(rem_minutes));
                                $('#tb1-seconds').text(pad(rem_seconds));
                                countDownSet_1(pad(rem_days), pad(rem_hours), pad(rem_minutes), pad(rem_seconds));
                                if (seconds <= 0) {
                                clearInterval(countDown_1);
                                $('#flash_sale>div').html('');
                                }
                                }

                                function countDownSet_1(days, hours, minutes, seconds) {
                                if (last_second != seconds) {
                                $('#tb1-seconds').addClass('cd-pop');
                                last_second = seconds;
                                } else {
                                $('#tb1-seconds').removeClass('cd-pop');
                                }
                                if (last_minute != minutes) {
                                $('#tb1-minutes').addClass('cd-pop');
                                last_minute = minutes;
                                } else {
                                $('#tb1-minutes').removeClass('cd-pop');
                                }
                                if (last_hour != hours) {
                                $('#tb1-hours').addClass('cd-pop');
                                last_hour = hours;
                                } else {
                                $('#tb1-hours').removeClass('cd-pop');
                                }
                                if (last_day != days) {
                                $('#tb1-days').addClass('cd-pop');
                                last_day = days;
                                } else {
                                $('#tb1-days').removeClass('cd-pop');
                                }
                                }

                                function pad(str) {
                                str = str.toString();
                                return str.length < 2 ? pad("0" + str, 2) : str;
                                }
                            </script2>
